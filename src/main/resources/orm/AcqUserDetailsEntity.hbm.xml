<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.acq.users.entity.AcqUserDetailsEntity" table="acq_terminal_users" catalog="acquiro_ver1">    	  
    	<id name="id" type="java.lang.Long">
            <column name="id" unique = "true"/>    
              <generator class="identity" />        
        </id>  
        <property name="phone" type="string">
            <column name="login_id"/>
        </property>
        <property name="orgId" type="java.lang.Long">
            <column name="org_id"/>
        </property>
        <property name="emailId" type="string">
            <column name="email_id"/>
        </property>
        <property name="rmn" type="string">
            <column name="rmn" />
        </property>
        <property name="otp" type="string">
            <column name="otp"/>
        </property>
         <property name="status" type="string">
            <column name="status"/>
        </property>
         <property name="password" type="string">
            <column name="password"/>
        </property>
        <property name="smsStatus" type="string">
            <column name="SMSSTATUS"/>
        </property>
        <property name="acquirerCode" type="string">
            <column name="AQUIRER_CODE"/>
        </property>
    </class>
</hibernate-mapping>