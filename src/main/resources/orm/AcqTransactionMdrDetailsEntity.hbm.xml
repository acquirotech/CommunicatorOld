<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.acq.users.entity.AcqTransactionMdrDetailsEntity" table="acq_transaction_add_details" catalog="acquiro_ver1">    	  
    	<id name="id" type="java.lang.Long">
            <column name="txn_id" />    
            <generator class="identity"/>        
        </id>  
       
        <property name="mdrCat" type="string">
            <column name="mdr_cat"/>
        </property>
        <property name="bankMdr" type="string">
            <column name="bank_mdr"/>
        </property>
        <property name="ezMdr" type="string">
            <column name="Acq_mdr"/>
        </property>
        <property name="serviceTax" type="string">
            <column name="servicetax"/>
        </property>
        <one-to-one name="detailsEntity" class="com.acq.users.entity.AcqTransactionDetailEntity" cascade="all" fetch="select"></one-to-one> 
       <!--  <one-to-one name="detailsEntity" class="com.ez.users.entity.EzTransactionDetailEntity" cascade="all" fetch="select"></one-to-one>  -->
    </class>
</hibernate-mapping>