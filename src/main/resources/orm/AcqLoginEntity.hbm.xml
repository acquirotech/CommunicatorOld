<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.acq.users.entity.AcqLoginEntity" table="acq_terminal_users" catalog="acquiro_ver1">    	  
    	<id name="userId" type="java.lang.Long">
            <column name="id" /> 
            <generator class="identity" />           
        </id>
        <property name="username" type="string">
            <column name="login_id" length="10" not-null="true" />
        </property>
        <property name="password" type="string">
        	<column name="password" length="100" not-null="true" />
        </property>
        <property name="status" type="string">
        	<column name="status" length="3" not-null="false" />
        </property>
          <property name="orgId" type="string">
        	<column name="org_id" length="20" not-null="false" />
        </property>       
  
   		<property name="userStatus" type="string">
        	<column name="USERSTATUS" />
        </property> 
        <property name="loyaltyStatus" type="string">
        	<column name="LOYALTYSTATUS" />
        </property> 
        <property name="walletStatus" type="string">
        	<column name="CLOSEWLLTSTATUS" />
        </property>
        <property name="switchType" type="string">
        	<column name="SWITCHTYPE" />
        </property>
        <property name="acquirerCode" type="string">
        	<column name="AQUIRER_CODE" />
        </property>
    </class>
</hibernate-mapping>